  _prompt: Simple solid fill shader. The color is exposed as a property.
  _cached: "Shader \"Unlit/SolidFill\"\n{\n    Properties\n    {\n        _Color
    (\"Color\", Color) = (1,1,1,1)\n    }\n\n    SubShader\n    {\n        Tags {\"Queue\"=\"Transparent\"
    \"RenderType\"=\"Opaque\"}\n\n        Pass\n        {\n            CGPROGRAM\n           
    #pragma vertex vert\n            #pragma fragment frag\n            #include
    \"UnityCG.cginc\"\n\n            struct appdata\n            {\n               
    float4 vertex : POSITION;\n            };\n\n            struct v2f\n           
    {\n                float4 vertex : SV_POSITION;\n            };\n\n           
    v2f vert (appdata v)\n            {\n                v2f o;\n               
    o.vertex = UnityObjectToClipPos(v.vertex);\n                return o;\n           
    }\n\n            fixed4 _Color;\n\n            fixed4 frag (v2f i) : SV_Target\n           
    {\n                return _Color;\n            }\n            ENDCG\n       
    }\n    }\n\n    FallBack \"Diffuse\"\n}"
